Public Sub uscita()

     'Alla chiusura del Cybertronic si spegne tutto
     If CP240.OPCData.IsConnected Then
     
         CP240.OPCData.items(PLCTAG_NM_IN_ARRESTO_IMM).Value = True

         Dim i As Integer
         For i = 1 To MAXMOTORI
             NMSetMotoreUscita i, False
         Next i
     
         CP240.OPCData.SOUpdate
     End If

     'Pezza per eliminare dal D.B. eventuali righe con dosaggi nulli
     Call EseguiSQL("DELETE FROM StoricoImpasto WHERE (ImpastoReale <= 300);")

     If (CP240.OPCData.IsConnected = True) Then
         CP240.OPCData.Disconnect
     End If
     SqlServerChiudi

     Call SendMessagetoPlus(PlusSendClose, 0)
     
     Call StopMessaging
     
     Unload CP240
     End
End Sub


Private Sub LoadImmaginiPulsantePlus(Index As Integer, Stato As StatoPulsantePlus)
	Dim prefisso As String
       
    On Error GoTo Errore

'selezione prefisso nome immagine

    Select Case Index

        Case TBB_STOPDOSAGGIO           'CmdNettiSiloStoricoSommaSalva(4)
