Private Sub cmbTargaCamion_Click()

    Dim rs As New adodb.Recordset

    On Error GoTo Errore

    If (cmbTargaCamion.text = "") Then
        Exit Sub
    End If

    If (cmbTargaCamionCurrValue = cmbTargaCamion.text) Then
        Exit Sub
    End If

    cmbTargaCamionCurrValue = cmbTargaCamion.text

    'ricerca l'Id della targa camion selezionata e associata al cliente selezionato nella AdoComboClientiCamion
    With rs
        Set .ActiveConnection = DBcon
        '20170221
        '.Source = "SELECT [IdCamion],[Targa] FROM [CYB500].[dbo].[Camion];"
        .Source = "SELECT [IdCamion],[Targa],[Portata] FROM [CYB500].[dbo].[Camion];"
        '
        .LockType = adLockReadOnly
        .CursorLocation = adUseClient
        .CursorType = adOpenForwardOnly
        .Open , DBcon
                
        If Not .BOF Then 'verifica se la tabella non e' vuota
                
            .MoveFirst
        
            Do Until .EOF
                If .Fields("Targa").Value = cmbTargaCamion.text Then
                    IdTargaCamionScaricoSilo = .Fields("IdCamion").Value
                    '20170221
                    If CDbl(.Fields("Portata").Value) > BilanciaPesaCamion.PesaCamionScalingKgMax Then
                        CP240.ProgressBil(3).max = CDbl(.Fields("Portata").Value)
                        CP240.ProgressBil(9).max = CDbl(.Fields("Portata").Value)
                        BilanciaPesaCamion.Sicurezza = CDbl(.Fields("Portata").Value) '20170224
                        CP240.LblEtichetta(21).caption = CStr(.Fields("Portata").Value) & " T"
                    ElseIf CDbl(.Fields("Portata").Value) > 0 Then
                        CP240.ProgressBil(3).max = BilanciaPesaCamion.PesaCamionScalingKgMax
                        CP240.ProgressBil(9).max = CDbl(.Fields("Portata").Value)
                        BilanciaPesaCamion.Sicurezza = CDbl(.Fields("Portata").Value)
                        CP240.LblEtichetta(21).caption = CStr(.Fields("Portata").Value) & " T"
                    Else
                        CP240.ProgressBil(3).max = BilanciaPesaCamion.PesaCamionScalingKgMax
                        CP240.ProgressBil(9).max = BilanciaPesaCamion.PesaCamionScalingKgMax
                        BilanciaPesaCamion.Sicurezza = BilanciaPesaCamion.PesaCamionScalingKgMax '20170224
                        CP240.LblEtichetta(21).caption = CStr(BilanciaPesaCamion.PesaCamionScalingKgMax) & " T"
                    End If
                    '
                    Exit Do
                End If
                .MoveNext
            Loop
        End If
        
        .Close
    End With

    Exit Sub
                                 
Errore:
    LogInserisci True, "CP240-008", CStr(Err.Number) + " [" + Err.description + "]"

End Sub

