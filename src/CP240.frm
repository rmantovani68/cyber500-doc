    End If

    If (pesata And Not PesataExtAttiva) Then
        Exit Sub
    End If

    If (pesata) Then
        CmdPesaEXT(Index).Picture = LoadResPicture("IDI_VALVOLAON", vbResIcon)
    Else
        CmdPesaEXT(Index).Picture = LoadResPicture("IDI_VALVOLA", vbResIcon)
    End If

    Select Case Index
        Case CompAggregato1
            CP240.OPCData.items(PLCTAG_DO_PesataEXTAgg1).Value = pesata
        Case CompAggregato2
            CP240.OPCData.items(PLCTAG_DO_PesataEXTAgg2).Value = pesata
        Case CompAggregato3
            CP240.OPCData.items(PLCTAG_DO_PesataEXTAgg3).Value = pesata
