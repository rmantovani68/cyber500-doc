Private Sub CmdStartStopGenerale_Click(Index As Integer)

    'Conferma operazione
    Dim buttonPressed As Integer

    Select Case Index
        Case 0
            'START

            'se nessun gruppo di avviamento automatico ridotto  stato selezionato si avvia l'automatico completo
            'altrimenti viene caricato il form per l'avviamento ridotto
            If NessunRidottoSelezionato Then
                buttonPressed = ShowMsgBox(strSiNo, vbOKCancel, vbExclamation, -1, -1, True)
                If (buttonPressed = vbCancel) Then
                    Exit Sub
                End If
                'AVVIAMENTO MOTORI
                Call SetMotorManagement(MotorSequenceEnum.StartAutoMotor)

            Else
                
                FormAvvRidotto.Show vbModal, CP240

                If SelezioneFormTipoAvvMotori <> vbOK Then Exit Sub

                    If (Not ProcediRidotto) Then
                        'AVVIAMENTO MOTORI
                        Call SetMotorManagement(MotorSequenceEnum.StartAutoMotor)
                    Else
                        'AVVIAMENTO MOTORI RIDOTTO
                        If (GruppoAvviamentoSelezionato(0)) Then
                            CP240.OPCData.items(PLCTAG_NM_MOTORI_GruppoAvviamentoRidotto).Value = 1
                        End If
                        If (GruppoAvviamentoSelezionato(1)) Then
                            CP240.OPCData.items(PLCTAG_NM_MOTORI_GruppoAvviamentoRidotto).Value = 2
                        End If
                        If (GruppoAvviamentoSelezionato(2)) Then
                            CP240.OPCData.items(PLCTAG_NM_MOTORI_GruppoAvviamentoRidotto).Value = 3
                        End If
                        Call SetMotorManagement(MotorSequenceEnum.StartAutoMotor1)
                    End If

                Debug.Print "CYBERTRONIC_PLUS PLCTAG_NM_MOTORI_GruppoAvviamentoRidotto"

            End If

            '20161230 ResetPID = True

        Case 1
            'STOP
            buttonPressed = ShowMsgBox(strSiNo, vbOKCancel, vbExclamation, -1, -1, True)
            If (buttonPressed = vbCancel) Then
                Exit Sub
            End If

            '20160226
            If (ListaTemperature(TempEntrataFiltro).valore > TemperaturaFiltroFreddo) Then
                'La temperatura dell'impianto non  abbastanza bassa per fare lo spegnimento:
                ' 1) Segnalo la situazione con un MessageBox e se si accetta, si prosegue comunque nello spegnimento
                ' 2) Se si vuole aspettare da qui esco e faccio partire un timer di attesa che, quando la temperatura lo permetter,
                '    far partire in automatico lo spegnimento.
                '    La fase di transizione  segnalata da un messaggio nella status bar. Qualunque operazione sui motori, ovviamente, la
                '    interrompe: stop immediato, start automatico, commutazione manuale o test
                buttonPressed = ShowMsgBox(LoadXLSString(1513), vbOKCancel, vbExclamation, -1, -1, True)
                If (buttonPressed = vbOK) Then
                    Call InizioAttesaStopAutomaticoMotori
                    Exit Sub
                End If
            End If
            '

            '20160226
            'Devo dare lo stop al ciclo di dosaggio
            'Call CmdStopDosaggio_Click
            '--
            'CP240.CmdAvvMotori(2).enabled = True
            'CP240.CmdAvvMotori(1).enabled = True
