Public Sub ComandiGestioneFumiTamburo(cmd_down As Boolean, cmd_up As Boolean)
    'Comandi
    If (cmd_down) Then
        If (Not GestioneFumiTamburo.Modulatore.min = GestioneFumiTamburo.Modulatore.posizione) Then
            GestioneFumiTamburo.Modulatore.Stato = ModulatoreStatusEnum.Modulatoredown
            ImgModulatore(IndexGestioneFumiTamburo).Visible = GestioneFumiTamburo.Modulatore.Stato = ModulatoreStatusEnum.Modulatoredown
        End If
    End If
    If (cmd_up) Then
        If (Not (GestioneFumiTamburo.Modulatore.max = GestioneFumiTamburo.Modulatore.posizione)) Then
            GestioneFumiTamburo.Modulatore.Stato = ModulatoreStatusEnum.ModulatoreUP
            ImgModulatore(IndexGestioneFumiTamburo + 1).Visible = GestioneFumiTamburo.Modulatore.Stato = ModulatoreStatusEnum.ModulatoreUP
        End If
    End If
    If  (Not cmd_up And Not cmd_down) Or 
		(cmd_down And GestioneFumiTamburo.Modulatore.min = GestioneFumiTamburo.Modulatore.posizione) Or 
		(cmd_up And GestioneFumiTamburo.Modulatore.max = GestioneFumiTamburo.Modulatore.posizione) Then
		
        GestioneFumiTamburo.Modulatore.Stato = ModulatoreStatusEnum.ModulatoreNone
        ImgModulatore(IndexGestioneFumiTamburo).Visible = False
        ImgModulatore(IndexGestioneFumiTamburo + 1).Visible = False
    End If
    'Comandi
End Sub
