Public Sub AbilitaDosaggioManuale(refreshInvPCL As Boolean)

    Dim indice As Integer

    '   Abilita il comando di pesatura
    For indice = 0 To compMax - 1
'20150624
'        If indice <> CompNonVagliato2 Then
'            CmdTrPesa(indice).Visible = PesaturaManuale
'        End If
        If indice = CompFiller2 Then
            CmdTrPesa(indice).Visible = PesaturaManuale And (GestioneFiller2 <> FillerSoloTramTamp)
'20151030
        ElseIf indice = CompFiller3 Then
            CmdTrPesa(indice).Visible = PesaturaManuale And (GestioneFiller3 <> FillerSoloTramTamp)
'
        ElseIf indice <> CompNonVagliato2 Then
            '20160421
            'CmdTrPesa(indice).Visible = PesaturaManuale
            CmdTrPesa(IIf(indice >= CompViatopScarMixer1, indice + OffsetPesViatopScarMixer, indice)).Visible = PesaturaManuale
            '20160421
        End If
'
    Next indice

    '   Abilita i comandi di scarico
    CmdScarica(0).Visible = PesaturaManuale
    CmdScarica(1).Visible = PesaturaManuale
    CmdScarica(2).Visible = PesaturaManuale
    CmdScarica(3).Visible = (PesaturaManuale And AbilitaRAP)
    CmdScarica(4).Visible = PesaturaManuale And InclusioneViatop
    '20160421
    CmdScarica(31).Visible = PesaturaManuale And BilanciaViatopScarMixer1.Presenza
    CmdScarica(32).Visible = PesaturaManuale And BilanciaViatopScarMixer2.Presenza
    CmdComprViatop(31).Visible = PesaturaManuale And BilanciaViatopScarMixer1.Presenza
    CmdComprViatop(32).Visible = PesaturaManuale And BilanciaViatopScarMixer2.Presenza
    '20160421
    CmdScarica(7).Visible = PesaturaManuale And InclusioneViatop

    CmdScarica(2).enabled = (Not BloccoSpruzzaturaAltaTemp)

    CmdScarica(ScarichiEnum.ScaricoMescolatore).Visible = PesaturaManuale
    CmdScarica(ScarichiEnum.ScaricoMescolatoreOn).Visible = PesaturaManuale
    CmdScarica(ScarichiEnum.ScaricoMescolatoreOn).enabled = PesaturaManuale And Not ListaMotori(MotoreMescolatore).ritorno
    
    CmdAddPesa(0).Visible = PesaturaManuale And InclusioneAcqua
    CmdAddPesa(1).Visible = PesaturaManuale And InclusioneAddMescolatore
    CmdAddPesa(2).Visible = PesaturaManuale And InclusioneAddBacinella
    CmdAddPesa(3).Visible = PesaturaManuale And InclusioneAddSacchi

    AutomaticoViatop = (Not PesaturaManuale)

    ManualePesaturaComponenti = compMax
    ManualeScaricoComponenti = ScaricoNiente
    
    If (PesaturaManuale) Then
        Call ArrestoEmergenzaDosaggio

        CP240.AniPushButtonDeflettore(10).Value = 2
        'Call ButtonSkipStartEnable '20160718
    Else
        ManualeAdditivi(0) = False
        ManualeAdditivi(1) = False
        ManualeAdditivi(2) = False
        ManualeAdditivi(3) = False

        CP240.AniPushButtonDeflettore(10).Value = 1
        
        'Call ButtonSkipStartEnable '20160718
    End If
    
    '20150409
    'Se la hardlock  rimossa o non c' comunicazione con Plus, il deflettore deve stare in automatico
    'If (HardKeyRemoved) Then
    If (HardKeyRemoved Or PlusCommunicationBroken) Then
    '
        CP240.AniPushButtonDeflettore(10).Value = 1
    End If
    
    If InclusioneBitume2 Or InclusioneBitume3 Then
        Call PosizionaBitume123
    End If

    Frame1(27).Visible = PesaturaManuale And InclusioneBitume3 And PlcSchiumato.Abilitazione
    
    If (refreshInvPCL) Then
        CP240.AbilitaInversionePCL
    End If

    Call PesataExtAbilita

    imgValvolaCisterne(4).enabled = PesaturaManuale

    '20161024
'    CmdTipoPesate(100).Visible = (PesaturaManuale And BilanciaAggregati.ProfiNet And ActiveUser >= OPERATOR)
'    CmdTipoPesate(101).Visible = (PesaturaManuale And BilanciaFiller.ProfiNet And ActiveUser >= OPERATOR)
'    CmdTipoPesate(102).Visible = (PesaturaManuale And BilanciaLegante.ProfiNet And ActiveUser >= OPERATOR)
'    CmdTipoPesate(103).Visible = (PesaturaManuale And BilanciaRAP.ProfiNet And ActiveUser >= OPERATOR) And AbilitaRAP
'    CmdTipoPesate(104).Visible = (PesaturaManuale And BilanciaViatop.ProfiNet And ActiveUser >= OPERATOR) And InclusioneViatop
'    CmdTipoPesate(105).Visible = (PesaturaManuale And BilanciaViatopScarMixer1.ProfiNet And ActiveUser >= OPERATOR) And BilanciaViatopScarMixer1.Presenza
'    CmdTipoPesate(106).Visible = (PesaturaManuale And BilanciaViatopScarMixer2.ProfiNet And ActiveUser >= OPERATOR) And BilanciaViatopScarMixer2.Presenza
    '

    UltimoImpastoCompletato = True

End Sub


