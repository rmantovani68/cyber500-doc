        Case CompAggregato5
            CP240.OPCData.items(PLCTAG_DO_PesataEXTAgg5).Value = pesata
        Case CompAggregato6
            CP240.OPCData.items(PLCTAG_DO_PesataEXTAgg6).Value = pesata
        Case CompNonVagliato
            CP240.OPCData.items(PLCTAG_DO_PesataEXTAggNV).Value = pesata
        Case CompNonVagliato2
            'CP240.OPCData.items(PLCTAG_DO_PesataEXTAggNV2).Value = pesata
        Case CompRAPAgg7
            CP240.OPCData.items(PLCTAG_DO_PesataEXTAgg7).Value = pesata     'RAP nel vaglio quando c'
        Case CompFiller1
            CP240.OPCData.items(PLCTAG_DO_PesataEXTFill1).Value = pesata
        Case CompFiller2
            CP240.OPCData.items(PLCTAG_DO_PesataEXTFill2).Value = pesata
        Case CompFiller3
            CP240.OPCData.items(PLCTAG_DO_PesataEXTFill3).Value = pesata
        
    End Select
    
    MemoriaPesataExtComando = pesata

End Sub

