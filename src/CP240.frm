Private Sub CmdAvviamentoBruciatoreCaldo_Click(Index As Integer)

    Dim tamburo As Integer
    Dim codiceerrore As Long
    Dim condizioni_autostart_pompa As Boolean '20160718

    tamburo = Index

    With ListaTamburi(tamburo)

        If .AvviamentoBruciatoreCaldo Then
            AllarmeCicalino = True
            Call ShowMsgBox(NoOperazione, vbOKOnly, vbExclamation, -1, -1, True)
            AllarmeCicalino = False
            Exit Sub
        End If

        .MemPortaModBrucASetAvvCaldo = True

        codiceerrore = VerificaCondizioniAvviamentoBruciatore(tamburo, True)
        If (codiceerrore <> 0 And (Not codiceerrore = 187) And (Not codiceerrore = 205) And (Not codiceerrore = 201) And (Not codiceerrore = 214)) Then
            .MemPortaModBrucASetAvvCaldo = False
            .AvviamentoBruciatoreCaldo = False

            AllarmeCicalino = True
            Call ShowMsgBox(LoadXLSString(codiceerrore), vbOKOnly, vbExclamation, -1, -1, True)
            AllarmeCicalino = False
            Exit Sub
        End If

        If (ShowMsgBox(strSiNo, vbOKCancel, vbExclamation, -1, -1, True) = vbCancel) Then
            .AvviamentoBruciatoreCaldo = False    '20160718
            Exit Sub
        End If
        '20160718
        condizioni_autostart_pompa = (MotorManagement = AutomaticMotor Or ListaTamburi(0).AvviamentoBruciatoreCaldo) And (ListaTamburi(0).SelezioneCombustibile = CombustibileGasolio) And Not ListaMotori(MotorePompaCombustibile).ritorno
        If (condizioni_autostart_pompa) Then
            FrmGestioneTimer.RitardoStartBruc.enabled = True
            CP240.OPCData.items(PLCTAG_DB2024_HMI_Start_Pompa_Combustibile_Caldo).Value = True
            .AvviamentoBruciatoreCaldo = True
        End If
        '20160718
        
        .AvviamentoBruciatoreCaldo = True
        
        '20161230 ResetPID = True
        If (Not condizioni_autostart_pompa) Then  '20160718
            .AvviamentoBruciatoreCaldo = True
            Call StartBruciatore(tamburo)
            If (tamburo = 0) Then
                CP240.OPCData.items(PLCTAG_NM_IN_START_RID_BRUC1).Value = True
            Else
                CP240.OPCData.items(PLCTAG_NM_IN_START_RID_BRUC2).Value = True
            End If
    
            FrmGestioneTimer.TmrCmdBruciatore.enabled = False
            FrmGestioneTimer.TmrCmdBruciatore.Interval = 500
            FrmGestioneTimer.TmrCmdBruciatore.enabled = True
        End If  '20160718
    End With

End Sub

