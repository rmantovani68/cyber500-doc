        Case TBB_LEGANTE, TBB_GRUPPO_CISTERNE                 'CmdMenuStartEsci(4)
            prefisso = "PLUS_IMG_CISTERNABITUME"

        Case TBB_MOTORI                  'CmdMenuStartEsci(5)
            prefisso = "PLUS_IMG_MOTORI"
                        
        Case TBB_EMULSIONE               'CmdMenuStartEsci(7)
            prefisso = "PLUS_IMG_CISTERNAEMULSIONE"
        
        Case TBB_COMBUSTIBILE            'CmdMenuStartEsci(8)
            prefisso = "PLUS_IMG_CISTERNACOMBUSTIBILE"
        
        Case TBB_PLCIO                   'CmdModalitaManutentore
            If PlcSimulation Then
                prefisso = "PLUS_IMG_TRAINING"
                imgPulsanteForm(Index).Picture = PlusImageList(0).ListImages(prefisso + PlusSuffissoFileImgPulsanteForm(default)).Picture
                Exit Sub
            Else
                prefisso = "PLUS_IMG_DIAGNOSTIC"
            End If
                                
        Case TBB_MANUTENZIONI
            prefisso = "PLUS_IMG_MANUTENZIONI"
            
        Case TBB_STORICOPREDOSAGGIO
            prefisso = "PLUS_IMG_STORICOPREDOSAGGIO"
        '20170220
        Case TBB_TOTALDOSINGRECIPE
            prefisso = "PLUS_IMG_TOT_RIC_DOS"
        '
        Case TBB_LOGIN
            prefisso = IIf((ActiveUser = NONE), "PLUS_IMG_LOGIN", "PLUS_IMG_LOGOFF")
            
        Case TBB_STORICOSILO
            prefisso = "PLUS_IMG_STORICOSILO"
        
        Case TBB_GRUPPO_STORICI
            prefisso = "PLUS_IMG_STORICO"
        
        Case Else
            Exit Sub
    End Select

    imgPulsanteForm(Index).Picture = PlusImageList(0).ListImages(prefisso + PlusSuffissoFileImgPulsanteForm(Stato)).Picture
            
    Exit Sub
Errore:
    LogInserisci True, "CP240-001", CStr(Err.Number) + " [" + Err.description + "]"

End Sub

Public Sub UpdatePulsantiForm()
        
Dim indice As Integer
        
    For indice = 0 To (TopBarButtonEnum.TBB_LAST - 1)
        If imgPulsanteForm(indice).enabled Then
            Call LoadImmaginiPulsantePlus(indice, StatoPulsantePlus.default)
        Else
            Call LoadImmaginiPulsantePlus(indice, StatoPulsantePlus.disabled)
        End If
    Next indice

    For indice = TopBarButtonSecondaryEnum.TBB_STORICOPREDOSAGGIO To (TopBarButtonSecondaryEnum.TBB_SECONDARYLAST - 1)
        If imgPulsanteForm(indice).enabled Then
            Call LoadImmaginiPulsantePlus(indice, StatoPulsantePlus.default)
        Else
            Call LoadImmaginiPulsantePlus(indice, StatoPulsantePlus.disabled)
        End If
    Next indice

End Sub

