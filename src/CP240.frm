Private Sub AdoComboClientiCamion_Change()

    Dim rs As New adodb.Recordset

    On Error GoTo Errore

    If (AdoComboClientiCamion.text = "") Then
        Exit Sub
    End If

    'ricerca l'Id del cliente selezionato nella AdoComboClientiCamion
    With rs
        Set .ActiveConnection = DBcon
        .Source = "SELECT [IdCliente],[Descrizione],[IdLOG] FROM [CYB500].[dbo].[Clienti];"
        .LockType = adLockReadOnly
        .CursorLocation = adUseClient
        .CursorType = adOpenForwardOnly
        .Open , DBcon
                
        If Not .BOF Then
                
            .MoveFirst
        
            Do Until .EOF
                If .Fields("Descrizione").Value = AdoComboClientiCamion.text Then
'                    IdClienteScaricoSilo = .Fields("IdCliente").Value
                    IdClienteScaricoSilo = .Fields("IdLOG").Value
                    Exit Do
                End If
                .MoveNext
            Loop
        End If
        
        .Close
    End With


'20160314
    Call RefillTargheCamion
                                 
    Exit Sub
                                 
Errore:
    LogInserisci True, "CP240-007", CStr(Err.Number) + " [" + Err.description + "]"
                                                                  
End Sub
