            MemPesataManualeViatopScarMixer2Attivata = True
        '20160421
    End Select
'

    Select Case Index

        Case CompAggregato1, CompAggregato2, CompAggregato3
            ' NON posso utilizzare il peso della bilancia per la coda del N.V. ho mischiato NV con Vagliato
            NvInManuale = 2

        Case CompAggregato4
        Case CompAggregato5
        Case CompAggregato6
        Case CompRAPAgg7
            ' NON posso utilizzare il peso della bilancia per la coda del N.V. ho mischiato NV con Vagliato
            NvInManuale = 2

        Case CompNonVagliato
            If (NvInManuale = 0) Then
                ' POSSO utilizzare il peso della bilancia per la coda del N.V. ho solo NV
                NvInManuale = 1
            End If

        Case CompFiller1
            FineRitardoConteggioF(0) = True
        Case CompFiller2
            FineRitardoConteggioF(1) = True
        Case CompFiller3
            FineRitardoConteggioF(2) = True

        Case CompLegante1
        Case CompLegante2
        Case CompLegante3
            CP240.OPCData.items(PLCTAG_DO_ContalitriPesataManuale).Value = True

        Case CompLeganteSoft
        Case CompLeganteHard

        Case CompViatop

        Case CompNonVagliato2
