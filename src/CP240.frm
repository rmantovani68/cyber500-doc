Public Sub SetUpBruciatore(tamburo As Integer)

    With ListaTamburi(tamburo)
        .InPreriscaldo = (.FiammaBruciatorePresente And Not TemperaturaLavoroFiltroOK)

        If .InPreriscaldo Then
            If .BruciatorePosizioneAccensione Then
                .ModulatoreBrucOnUp = True
                .ModulatoreBrucOnDown = False
            Else
                .ModulatoreBrucOnUp = False
                .ModulatoreBrucOnDown = False
            End If
        Else
            .ModulatoreBrucOnUp = True
            .ModulatoreBrucOnDown = False
        End If
    End With

End Sub

Public Sub SetDownBruciatore(tamburo As Integer)

    With ListaTamburi(tamburo)
        .ModulatoreBrucOnDown = True
        .ModulatoreBrucOnUp = False
    End With

End Sub

Private Sub CmdUpDownBruc_MouseDown(Index As Integer, Button As Integer, Shift As Integer, x As Single, Y As Single)

    Select Case Index
        Case 0      'Pulsante +
            Call SetUpBruciatore(0)
        Case 1      'Pulsante -
            Call SetDownBruciatore(0)
        Case 2      'Pulsante +
            Call SetUpBruciatore(1)
        Case 3      'Pulsante -
            Call SetDownBruciatore(1)
        '20161128
        Case IndexGestioneFumiTamburo
            Call ComandiGestioneFumiTamburo(True, False)
        Case (IndexGestioneFumiTamburo + 1)
            Call ComandiGestioneFumiTamburo(False, True)
        '20161128
        '20161130
        Case 4
            Call ComandiGestioneVelTamburo(False, True)
        Case 5
            Call ComandiGestioneVelTamburo(True, False)
        '20161130
    End Select

End Sub

Private Sub CmdUpDownBruc_MouseUp(Index As Integer, Button As Integer, Shift As Integer, x As Single, Y As Single)
    If (Index = 0 Or Index = 1) Then    'Pulsante + e -
        ListaTamburi(0).ModulatoreBrucOnUp = False
