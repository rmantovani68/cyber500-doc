Private Sub CmdUpDownAriaFiltro_LostFocus(Index As Integer)
    If ModulatoreAspirazioneFiltro.manuale Then
        If (Index = 0 Or Index = 1) Then
            If ( _
                (Index = 0 And ModulatoreAspirazioneFiltro.Stato = ModulatoreStatusEnum.ModulatoreUP) Or _
                (Index = 1 And ModulatoreAspirazioneFiltro.Stato = ModulatoreStatusEnum.Modulatoredown) _
            ) Then
                ModulatoreAspirazioneFiltro.Stato = ModulatoreStatusEnum.ModulatoreNone
            End If
        End If
    End If

    If ManualeAriaFredda Then
        If (Index = 2 Or Index = 3) Then
            If ( _
                (Index = 2 And ModulatoreAriaFreddaFiltro.Stato = ModulatoreStatusEnum.ModulatoreUP) Or _
                (Index = 3 And ModulatoreAriaFreddaFiltro.Stato = ModulatoreStatusEnum.Modulatoredown) _
            ) Then
                ModulatoreAriaFreddaFiltro.Stato = ModulatoreStatusEnum.ModulatoreNone
            End If
        End If
    End If
    
    If ListaTamburi(0).ModulatoreFumiTamburo.manuale Then
        If (Index = 4 Or Index = 5) Then
            If (Index = 4 And ListaTamburi(0).ModulatoreFumiTamburo.Stato = ModulatoreStatusEnum.ModulatoreUP) Or (Index = 5 And ListaTamburi(0).ModulatoreFumiTamburo.Stato = ModulatoreStatusEnum.Modulatoredown) Then
                ListaTamburi(0).ModulatoreFumiTamburo.Stato = ModulatoreStatusEnum.ModulatoreNone
            End If
        End If
    End If
    If ListaTamburi(1).ModulatoreFumiTamburo.manuale Then
        If (Index = 6 Or Index = 7) Then
            If (Index = 6 And ListaTamburi(1).ModulatoreFumiTamburo.Stato = ModulatoreStatusEnum.ModulatoreUP) Or (Index = 7 And ListaTamburi(1).ModulatoreFumiTamburo.Stato = ModulatoreStatusEnum.Modulatoredown) Then
                ListaTamburi(1).ModulatoreFumiTamburo.Stato = ModulatoreStatusEnum.ModulatoreNone
            End If
        End If
    End If

