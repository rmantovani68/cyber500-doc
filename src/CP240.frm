Public Sub VisualizzaStatoComunicazione()

    Dim connesso As Boolean

On Error GoTo Errore

    'PLC PRINCIPALE

    connesso = (CP240.OPCData.IsConnected And Not MancanzaComunicazione)
    If (connesso) Then
        connesso = (GetQuality(CP240.OPCData.items.item(0).quality) = STATOOK)
    End If

    If (StatoConnessionePlcPrincipale <> connesso Or Not VisualizzazioneConnessionePlcGiaFatta) Then
        StatoConnessionePlcPrincipale = connesso
        StatoConnessionePlcPrincipale_change
    End If


    'PLC CISTERNE

    If (CistGestione.Gestione = GestionePLC) Then
        connesso = (CP240.OPCDataCisterne.items.count > 0)
        If (connesso) Then
            connesso = (GetQuality(CP240.OPCDataCisterne.items.item(0).quality) = STATOOK)
        End If

        If (StatoConnessionePlcCisterne <> connesso Or Not VisualizzazioneConnessionePlcGiaFatta) Then
            StatoConnessionePlcCisterne = connesso
            StatoConnessionePlcCisterne_change
        End If
'20161018
    Else
        Call CP240StatusBar_Change(STB_PLCCISTERNE, 99)
    End If
    

    'PLC SCHIUMATO

    If (PlcSchiumato.Abilitazione) Then
        connesso = PlcSchiumatoConnesso

        If (StatoConnessionePlcSchiumato <> connesso Or Not VisualizzazioneConnessionePlcGiaFatta) Then
            StatoConnessionePlcSchiumato = connesso
            StatoConnessionePlcSchiumato_change
        End If
'20161018
    Else
        Call CP240StatusBar_Change(STB_PLCSCHIUMATO, 99)
    End If

'20160506
    'AQUABLACK
    If InclusioneAquablack Then
        connesso = PlcAquablackConnesso
        If (StatoConnessionePlcAquablack <> connesso Or Not VisualizzazioneConnessionePlcGiaFatta) Then
            StatoConnessionePlcAquablack = connesso
            StatoConnessionePlcAquablack_change
        End If
'20161018
    Else
        Call CP240StatusBar_Change(STB_AQUABLACK, 99)
    End If
'

    VisualizzazioneConnessionePlcGiaFatta = True

    Exit Sub

Errore:
    LogInserisci True, "CP240-003", CStr(Err.Number) + " [" + Err.description + "]"
End Sub

