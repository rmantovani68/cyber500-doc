Public Sub RefreshBenna()

    If (Not InclusioneBenna) Then
        Exit Sub
    End If

    Select Case BennaVisualizzata
        Case 0:
            If (VisualizzaBenna) Then
                ImgBenna(0).Picture = LoadResPicture("IDB_BENNAVUOTA", vbResBitmap)
            Else
                ImgBenna(0).Picture = LoadResPicture("IDB_NAVETTAVUOTA", vbResBitmap)
            End If
        Case 1:
            If (VisualizzaBenna) Then
                ImgBenna(0).Picture = LoadResPicture("IDB_BENNACARICA", vbResBitmap)
            Else
                ImgBenna(0).Picture = LoadResPicture("IDB_NAVETTACARICA", vbResBitmap)
            End If
        Case 2, 3:
            ImgBenna(0).Picture = LoadResPicture("IDB_CAMION", vbResBitmap)
    End Select

End Sub

Public Sub ShowBenna(carica As Boolean)

    Dim newBennaVisualizzata As Integer

    If (Not InclusioneBenna) Then
        Exit Sub
    End If

    If (DestinazioneSilo = 11 And VisualizzaCamionPerSiloDiretto) Then
        'Nel silo diretto c' chi preferisce vedere il camioncino
        If (carica) Then
            newBennaVisualizzata = 3
        Else
            newBennaVisualizzata = 2
        End If
        CP240.ImgBenna(0).Visible = True
    Else
        If (carica) Then
            newBennaVisualizzata = 1
        Else
            newBennaVisualizzata = 0
        End If
        CP240.ImgBenna(0).Visible = BennaFineCorsaInf And (Not InclusioneSilo2S7 Or (InclusioneSilo2S7 And BennaFineCorsaInfAsse2))
        '
    End If

    If (BennaVisualizzata <> newBennaVisualizzata) Then
        BennaVisualizzata = newBennaVisualizzata
        Call RefreshBenna
    End If

End Sub


