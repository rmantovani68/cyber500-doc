Private Sub CmdTrPesa_MouseDown(Index As Integer, Button As Integer, Shift As Integer, x As Single, Y As Single)

    If (Not PesaturaManuale) Then
        Exit Sub
    End If

    CmdTrPesa(Index).Picture = LoadResPicture("IDI_VALVOLAON", vbResIcon)

    If Index < 100 Then
        '20160421
        'ManualePesaturaComponenti = Index
        ManualePesaturaComponenti = IIf(Index >= CompViatopScarMixer2, Index - OffsetPesViatopScarMixer, Index)
        '20160421
    Else
        ManualePesaturaComponenti = Index - 100
    End If

    '20160421
    'CodiceCompScManuale = Index
    CodiceCompScManuale = IIf(Index >= CompViatopScarMixer2, Index - OffsetPesViatopScarMixer, Index)
    '20160421

'memorizza il peso della bilancia corrispondente all'inizio della pesata manuale
    Select Case Index
        '20170223
        'Case CompAggregato1, CompAggregato2, CompAggregato3, CompAggregato4, CompAggregato5, CompAggregato6, CompNonVagliato
        Case CompAggregato1, CompAggregato2, CompAggregato3, CompAggregato4, CompAggregato5, CompAggregato6, CompNonVagliato, CompGrafRAPAgg7
'
            PesoAggregatiManuale = BilanciaAggregati.Peso
            MemPesataManualeAggregatiAttivata = True
        Case CompFiller1, CompFiller2, CompFiller3
            PesoFillerManuale = BilanciaFiller.Peso
            MemPesataManualeFillerAttivata = True
        Case CompLegante1, CompLegante2, CompLegante3
            PesoBitumeManuale = BilanciaLegante.Peso
            MemPesataManualeBitumeAttivata = True
        Case CompRAP
            PesoRiciclatoManuale = BilanciaRAP.Peso
            MemPesataManualeRiciclatoAttivata = True
        Case CompRAPSiwa
            'questa bilancia fa eccezione, in quanto scarica direttamente nel mescolatore
            MemPesataManualeRiciclatoAttivata = True
        Case CompViatop
            PesoViatopManuale = BilanciaViatop.Peso
            MemPesataManualeViatopAttivata = True
        '20160421
        Case CompViatopScarMixer1 + OffsetPesViatopScarMixer
