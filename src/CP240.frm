Private Sub tmrRicTrasNET_Timer(Index As Integer)

    On Error GoTo Errore

    VisualizzaStatoComunicazione
    Select Case Index

        Case 0   'TIMER CONTROLLO NET.
            If (GetQuality(CP240.OPCData.items.item(0).quality) <> STATOOK And ProgrammaAvviato) Then
                CP240.OPCData.Disconnect
'                PlcEffettuaConnessione
            End If

            If PlcSchiumato.Abilitazione Then
                If Not CP240.OPCDataSchiumato.IsConnected Or (GetQuality(CP240.OPCDataSchiumato.items.item(0).quality) <> STATOOK And ProgrammaAvviato) Then
                    CP240.OPCDataSchiumato.Disconnect
'                    CP240.OPCDataSchiumato.Connect
                End If
            End If

        Case 1   'TRASMISSIONE DATI.
            If (BooleanModificato(connessionestatoOK, (GetQuality(CP240.OPCData.items.item(0).quality) = STATOOK), fatto)) Then
                 If (connessionestatoOK And ProgrammaAvviato) Then
                     FrmGestioneTimer.RitardoRitrasmPar.enabled = True
                 End If
            End If
            fatto = True

            Call VideataPrincipale

            If (DEMO_VERSION) Then
                Dim ChangedItems As OPCDataItems
                opcData_OnDataChanged 1, ChangedItems
            End If

            If (Not CP240.OPCData.IsConnected) Then
                Exit Sub
            End If

            Call PlcOutDigitali
            Call PlcOutAnalogici
            
            Call SiloS7Scrivi

            Call CisterneScriviDatiPLC

            Call PLCAquablack_ScriviTag '20160729
            
        Case 3  'Errore invio parametri al PLC
            CP240.tmrRicTrasNET(3).enabled = False
            EnableControlloComunicazione = True
    End Select

    Exit Sub
Errore:
    LogInserisci True, "CP240-005", CStr(Err.Number) + " [" + Err.description + "]"
End Sub

