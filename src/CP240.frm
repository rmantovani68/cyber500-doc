Public Sub ManageTopBarButton(Button As Integer)

    Dim buttonPressed As Integer
    Dim UpdateFrame As Boolean

    If Not ParallelDrum Then
        Frame1_2_Visible = False
        Frame1_3_Visible = False
        Frame1_24_Visible = False
        Frame1_20_Visible = False
        Frame1_36_Visible = False
        Frame1_39_Visible = False
        Frame1_43_Visible = False '20150505
    End If
    
    Select Case Button
    
        Case TBB_STOPDOSAGGIO
            buttonPressed = ShowMsgBox(LoadXLSString(138), vbOKCancel, vbExclamation, -1, -1, True)
            If (buttonPressed = vbOK) Then
                Call ArrestoEmergenzaDosaggio
            End If

        Case TBB_NETTI
            imgPulsanteForm(TBB_DOSAGGIO).enabled = False
            imgPulsanteForm(TBB_PREDOSAGGIO).enabled = False
            
            Unload FrmNetti
            FrmNetti.Hide
            FrmNetti.Show vbModeless, Me
            
        Case TBB_SILO
            imgPulsanteForm(TBB_DOSAGGIO).enabled = False
            imgPulsanteForm(TBB_PREDOSAGGIO).enabled = False
            
            Unload FrmSiloGenerale
            FrmSiloGenerale.Hide
            FrmSiloGenerale.ShowMe Me
            
        Case TBB_SIRENA
            'Gestito da evento MouseDown

        Case TBB_STORICOPREDOSAGGIO
            CmdStartDosaggioLock = True
            Call SendMessagetoPlus(PlusSendShowStoricoPredosaggio, 0)
            Exit Sub

        Case TBB_STORICO
            CmdStartDosaggioLock = True
            Call SendMessagetoPlus(PlusSendShowSTORICO, 0)
            Exit Sub
        
        Case TBB_STORICOIMPMANUALI
            CmdStartDosaggioLock = True
            Call SendMessagetoPlus(PlusSendShowSTORICOIMPMANUALI, 0)
            Exit Sub

        Case TBB_TOTALI
            Call SendMessagetoPlus(PlusSendShowTOTALI, 0)
            Exit Sub

        Case TBB_ALLARMI
            Call SendMessagetoPlus(PlusSendShowALLARMI, 0)
            Exit Sub

        '20150820
        Case TBB_DOSINGMATERIALS
            Call SendMessagetoPlus(PlusSendShowDosingMaterial, 0)
            Exit Sub
        '
        '20150820
        Case TBB_TOTALDOSINGRECIPE
            Call SendMessagetoPlus(PlusSendShowDosingHistoryByDay, 0)
            Exit Sub
        '

        Case TBB_TREND
           CmdStartDosaggioLock = True
            Call SendMessagetoPlus(PlusSendShowTREND, 0)
            Exit Sub
            
        Case TBB_ENERGIA
            Call SendMessagetoPlus(PlusSendShowENERGIA, 0)
            Exit Sub

        Case TBB_PLCIO
            If (Not PlcSimulation) Then
                Call SendMessagetoPlus(PlusSendShowPLCIO, 0)
                Exit Sub
            End If

        Case TBB_PARAMETRI
            Call SendMessagetoPlus(PlusSendShowPARAMETRI, 0)
            Exit Sub

        Case TBB_HELP
            VisualizzaHelp Me, HELP_PRINCIPALE

        Case TBB_ABOUT
            '20160502
            'Call SendMessagetoPlus(PlusSendSWVersion, CStr(app.Major) + "." + CStr(app.Minor) + "." + CStr(app.Revision) + "." + CStr(BUILDNUMBER))
            '
            Call SendMessagetoPlus(PlusSendShowABOUT, 0)
            Exit Sub

        Case TBB_EXIT
            '20160318
            If (StatoConnessionePlcPrincipale) Then
            '
                'Non posso uscire se ho il predosaggio attivo
                Dim Pred As Integer
                For Pred = 1 To MAXPREDOSATORI
                    If (ListaPredosatori(Pred - 1).motore.uscita) Then
                        AllarmeCicalino = True
                        buttonPressed = MsgBox(NoOperazione, vbOKOnly + vbExclamation, "MARINI")
                        AllarmeCicalino = False
                        Exit Sub
                    End If
                Next Pred
                For Pred = 1 To MAXPREDOSATORIRICICLATO
                    If (ListaPredosatoriRic(Pred - 1).motore.uscita) Then
                        AllarmeCicalino = True
                        buttonPressed = MsgBox(NoOperazione, vbOKOnly + vbExclamation, "MARINI")
                        AllarmeCicalino = False
                        Exit Sub
                    End If
                Next Pred
                'Non posso uscire se ho il dosaggio attivo o il bruciatore acceso
                If ( _
                    DosaggioInCorso Or _
                    ListaTamburi(0).FiammaBruciatorePresente Or ListaTamburi(1).FiammaBruciatorePresente Or _
                    FrmSiwarexParaVisibile _
                ) Then
                    AllarmeCicalino = True
                    buttonPressed = MsgBox(NoOperazione, vbOKOnly + vbExclamation, "MARINI")
                    AllarmeCicalino = False
                    Exit Sub
                End If
            End If

            'Conferma Uscita Programma
            buttonPressed = ShowMsgBox(LoadXLSString(107), vbOKCancel, vbExclamation, -1, -1, True)

            If (buttonPressed = vbOK) Then
                ChiusuraApplicazione = True
                
                Call SendMessagetoPlus(PlusSendClose, 0)
                FrmGestioneTimer.AttesaSpegnimento.enabled = True
            End If
            
        Case TBB_DOSAGGIO
            Frame1_2_Visible = True
            UpdateFrame = True
            Frame1_3_Visible = False
            Frame1_20_Visible = False
        
        Case TBB_PREDOSAGGIO
            Frame1_3_Visible = True
            UpdateFrame = True
            Frame1_2_Visible = False
            Frame1_20_Visible = False

        Case TBB_GRUPPO_CISTERNE
            If (CistGestione.NumCisterneCombustibile = 0 And CistGestione.NumCisterneEmulsione = 0) Then
                Frame1_39_Visible = False
                Frame1_36_Visible = False
'20151027
'                Frame1_24_Visible = True
                Frame1_24_Visible = False
