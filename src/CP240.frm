            prefisso = "PLUS_IMG_STORICOIMPMANUALI"
                        
        Case TBB_TOTALI                  'CmdNettiSiloStoricoSommaSalva(3)
            prefisso = "PLUS_IMG_GESTIONEMATERIALI"
                    
'        Case TBB_CONSUMOMAT              'CmdNettiSiloStoricoSommaSalva(19)
        
        Case TBB_ALLARMI                 'CmdStoricoAllarmi
            prefisso = "PLUS_IMG_STORICOALLARMI"
                        
        '20150820
        Case TBB_DOSINGMATERIALS
            '20160512
            'prefisso = "PLUS_IMG_GESTIONEMATERIALI"
            prefisso = "PLUS_IMG_STORICOMATERIALI"
            '
        '

        Case TBB_TREND                   'CmdNettiSiloStoricoSommaSalva(25)
            prefisso = "PLUS_IMG_GRAPH"
                        
        Case TBB_ENERGIA                 'CmdNettiSiloStoricoSommaSalva(10)
            prefisso = "PLUS_IMG_ENERGIA"
                        
        Case TBB_PARAMETRI               'CmdNettiSiloStoricoSommaSalva(21)
            prefisso = "PLUS_IMG_PARAMETERS"
                        
        Case TBB_HELP                    'CmdNettiSiloStoricoSommaSalva(17)
            prefisso = "PLUS_IMG_HELP"
                        
        Case TBB_ABOUT                   'CmdNettiSiloStoricoSommaSalva(6)
            prefisso = "PLUS_IMG_ABOUT"

        Case TBB_EXIT                    'CmdMenuStartEsci(0)
            prefisso = "PLUS_IMG_EXIT"
                                    
        Case TBB_DOSAGGIO                'CmdMenuStartEsci(1)
            prefisso = "PLUS_IMG_DOSAGGIO"

