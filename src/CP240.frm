        Case TBB_LEGANTE, TBB_EMULSIONE, TBB_COMBUSTIBILE

        Case Else
            Frame1(47).Visible = False
            Frame1(48).Visible = False
    End Select
    
    nextButtonIndex = 0

    '20160512
    'Intercetto movimento mouse x reset timer utente
